<% provide :title, t("user.index.title") %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1><%= t "user.index.header" %></h1>
    <ul class="users">
      <div class="text-center">
        <%= will_paginate %>
      </div>
      <% @users.each do |user| %>
      <%= render user %>
      <% end %>
      <div class="text-center">
        <%= will_paginate %>
      </div>
    </ul>
  </div>
</div>
